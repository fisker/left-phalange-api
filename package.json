{
  "name": "left-phalange-api",
  "version": "1.1.6",
  "description": "CommonJS, ESModule, INI, JSON, JSON5, TOML, YAML data loader, parser and stringifier",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "repository": "fisker/left-phalange-api",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "keywords": [
    "data",
    "json",
    "yaml",
    "json5",
    "toml",
    "ini",
    "load",
    "loader",
    "parse",
    "parser",
    "stringifier",
    "stringify"
  ],
  "license": "MIT",
  "files": [
    "lib"
  ],
  "dependencies": {
    "@iarna/toml": "^2.2.3",
    "ansi-colors": "^4.1.1",
    "esm": "^3.2.25",
    "import-fresh": "^3.1.0",
    "ini": "^1.3.5",
    "js-yaml": "^3.13.1",
    "json5": "^2.1.0",
    "parse-json": "^5.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  },
  "scripts": {
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s lint build format test dist",
    "test": "ava",
    "test-coverage": "nyc ava"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "sideEffects": false,
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@fisker/commitlint-config": "^1.1.2",
    "@fisker/eslint-config": "^1.2.10",
    "@fisker/husky-config": "^1.1.1",
    "@fisker/lint-staged-config": "^1.0.5",
    "@fisker/prettier-config": "^1.0.18",
    "ava": "^2.2.0",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "eslint": "^6.1.0",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.3",
    "markdownlint-cli": "^0.18.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rollup": "^1.19.4",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-filesize": "^6.2.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "temp-write": "^4.0.0"
  },
  "nyc": {
    "require": "esm",
    "reporter": [
      "lcov",
      "text"
    ],
    "include": [
      "src/**"
    ]
  },
  "ava": {
    "require": "esm",
    "verbose": true,
    "helpers": [
      "test/fixtures/**/*",
      "test/helpers/**/*"
    ]
  }
}
